local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/Loco-CTO/UI-Library/main/VisionLibV2/source.lua'))()

Window = Library:Create({
	Name = "Awaken#6969 MiscRelease",
	Footer = "By Awaken#6969 MiscRelease",
	ToggleKey = Enum.KeyCode.RightShift,
	LoadedCallback = function()
		Window:TaskBarOnly(false)
	end,
	KeySystem = false,
	Key = "123456",
	MaxAttempts = 5,
	DiscordLink = nil,
})

Window:ChangeTogglekey(Enum.KeyCode.RightShift)

local Tab = Window:Tab({
	Name = "Main",
	Icon = "rbxassetid://11396131982",
	Color = Color3.new(0.474509, 0.474509, 0.474509)
})

local Section1 = Tab:Section({
	Name = "Awaken#6969 MiscRelease"
})


Section1:Button({
	Name = "Teleport Maker",
	Callback = function()
		loadstring(game:HttpGet"https://raw.githubusercontent.com/Awakenchan/Misc-Release/main/Tp%20Maker")()
		Library:Notify({
			Name = "Script:",
			Text = "Script you ran is open source, script copied to your clipboard"..setclipboard("https://raw.githubusercontent.com/Awakenchan/Misc-Release/main/Tp%20Maker"),
			Icon = "rbxassetid://11401835376",
			Duration = 3
		})
	end
})

