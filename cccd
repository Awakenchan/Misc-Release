           print('not spawned')
           print('Server hopping')
        task.wait(4)
        Teleport()
        task.wait(50)
    else
        Fluent:Notify({
            Title = "Information:",
            Content = 'Flags havent spawned in this server yet, will be retrying after 15sec',
            Duration = 5
        })
        task.wait(15)
    end
end
end)
---------------------------CONFIG TAB---------------------------
moneyValue:GetPropertyChangedSignal("Text"):Connect(function() 
local num = string.gsub(moneyValue.Text, "RP.", "")
num = string.gsub(num, ",", "")
newMoney = tonumber(num) - oldMoney
end)
local function sendMoneyLog()
local url = thereurl or "default"
local data = {
["content"] = "",
["username"] = "awakenkn",
["avatar_url"] = "https://cdn.discordapp.com/attachments/1129303008950362203/1131670531503042680/icegif-796.gif",
["embeds"] = {
{
  ["color"] = 8163759,
  ["author"] = {
     ["name"] = "awakenkn Hub",
     ["url"] = "",
  },
  ["footer"] = {
     ["text"] = "by awakenkn | best cdid script!",
  },
  ["description"] = ("**Truck Farm Money Earned: %s"):format(newMoney)..'**',
  ["type"] = "rich",
}
},
}
local newdata = game:GetService("HttpService"):JSONEncode(data)
local headers = {
["content-type"] = "application/json"
}
request = http_request or request or HttpPost
local moneylog = {Url = url, Body = newdata, Method = "POST", Headers = headers}
request(moneylog)
end
local enterwebhook = Tabs.Config:AddInput("enterwebhook", {
Title = "Enter Webhook",
Default = "https://discord.com/api/webhooks/1212112121212121/",
Placeholder = "https://discord.com/api/webhooks/1212112121212121/",
Numeric = false, -- Only allows numbers
Finished = true, -- Only calls callback when you press enter
Callback = function(value)
  thereurl = value
end
})
enterwebhook:OnChanged(function(value)
thereurl = value
end)
local TruckfarmCounter = Tabs.Config:AddToggle("TruckfarmCounter", {Title = "Truck Farm Counter", Default = false })
TruckfarmCounter:OnChanged(function(value)
local moneyValue = game:GetService("Players").localPlayer.PlayerGui.Main.Container.Hub.CashFrame.Frame.TextLabel
getgenv().webhookTime = timeva --default
moneyValue:GetPropertyChangedSignal("Text"):Connect(function() -- updating the money
local num = string.gsub(moneyValue.Text, "RP.", "")
num = string.gsub(num, ",", "")
newMoney = tonumber(num) - oldMoney
end)
task.spawn(function()
while task.wait(getgenv().webhookTime) do
sendMoneyLog()
end
end)
end)
