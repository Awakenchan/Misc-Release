local OfficeFarm = Tabs.Farm:AddToggle("OfficeFarm", {Title = "Office Farm", Default = false })
OfficeFarm:OnChanged(function(value)
local button = game:GetService("Players").localPlayer.PlayerGui.Job.Components.Container.Office.Frame.SubmitButton
local events = {"MouseButton1Click", "MouseButton1Down", "Activated"}
getgenv().Officefarm = value
if getgenv().Officefarm then
task.spawn(getofficejob)
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Etc.Job.Office.Starter.WorldPivot
task.wait(0.6)
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Etc.Job.Office.Starter.WorldPivot
task.wait(1)
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Etc.Job.Office.Starter.WorldPivot
task.wait(1)
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Etc.Job.Office.Starter.WorldPivot
task.wait(1)
game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Etc.Job.Office.Starter.WorldPivot
fireproximityprompt(game:GetService("Workspace").Etc.Job.Office.Starter.Prompt)
end
while getgenv().Officefarm do task.wait()
str = game.Players.localPlayer.PlayerGui.Job.Components.Container.Office.Frame.Question.Text
str = str:gsub("=", "")
str = str:gsub(" ", "")
if string.find(str, "+") then
local first_num = tonumber(string.sub(str, 1, string.find(str, "+")-1))
local second_num = tonumber(string.sub(str, string.find(str, "+")+1))
result = first_num + second_num
elseif string.find(str, "-") then
local first_num = tonumber(string.sub(str, 1, string.find(str, "-")-1))
local second_num = tonumber(string.sub(str, string.find(str, "-")+1))
result = first_num - second_num
end
game.Players.localPlayer.PlayerGui.Job.Components.Container.Office.Frame.TextBox.Text = tostring(result)
task.wait(0.7)
for i,v in pairs(events) do
for i,v in pairs(getconnections(button[v])) do
v:Fire()
end
end
end
end)
