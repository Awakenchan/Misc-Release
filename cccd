getgenv().flagfarms3 = value
end)
local flagconfig = Tabs.Config:AddToggle("flagconfig", {Title = "Auto Open Flag box", Default = false })
flagconfig:OnChanged(function(value)
getgenv().flagfarms2 = value
while getgenv().flagfarms2 do task.wait()
    game:GetService("ReplicatedStorage"):WaitForChild("NetworkContainer"):WaitForChild("RemoteEvents"):WaitForChild("BoxEvent"):FireServer()
end
end)

